        <action
            android:id="@+id/action_statusBar_to_statusBarTile"
            app:destination="@id/statusBarTile"
            app:enterAnim="@anim/fragment_enter"
            app:exitAnim="@anim/fragment_exit"
            app:popEnterAnim="@anim/fragment_enter_pop"
            app:popExitAnim="@anim/fragment_exit_pop" />
        <action
            android:id="@+id/action_statusBar_to_statusBarClock"
            app:destination="@id/statusBarClock"
            app:enterAnim="@anim/fragment_enter"
            app:exitAnim="@anim/fragment_exit"
            app:popEnterAnim="@anim/fragment_enter_pop"
            app:popExitAnim="@anim/fragment_exit_pop" />
        <action
            android:id="@+id/action_statusBar_to_statusBarIcon"
            app:destination="@id/statusBarIcon"
            app:enterAnim="@anim/fragment_enter"
            app:exitAnim="@anim/fragment_exit"
            app:popEnterAnim="@anim/fragment_enter_pop"
            app:popExitAnim="@anim/fragment_exit_pop" />
        <action
            android:id="@+id/action_statusBar_to_statusBarDate"
            app:destination="@id/statusBarDate"
            app:enterAnim="@anim/fragment_enter"
            app:exitAnim="@anim/fragment_exit"
            app:popEnterAnim="@anim/fragment_enter_pop"
            app:popExitAnim="@anim/fragment_exit_pop" />
    </fragment>
    <fragment
        android:id="@+id/miscellaneous"
        android:name="com.luckyzyx.luckytool.ui.fragment.Miscellaneous"
        android:label="Miscellaneous" >
        <action
            android:id="@+id/action_miscellaneous_to_powerMenu"
            app:destination="@id/powerMenu"
            app:enterAnim="@anim/fragment_enter"
            app:exitAnim="@anim/fragment_exit"
            app:popEnterAnim="@anim/fragment_enter_pop"
            app:popExitAnim="@anim/fragment_exit_pop" />
    </fragment>
    <fragment
        android:id="@+id/powerMenu"
        android:name="com.luckyzyx.luckytool.ui.fragment.PowerMenu"
        android:label="PowerMenu" />
    <fragment
        android:id="@+id/alphaBackupPro"
        android:name="com.luckyzyx.luckytool.ui.fragment.AlphaBackupPro"
        android:label="AlphaBackupPro" />
    <fragment
        android:id="@+id/statusBarDate"
        android:name="com.luckyzyx.luckytool.ui.fragment.StatusBarDate"
        android:label="StatusBarDate" />

</navigation>